epm_autoremove()
{
case $PMTYPE in
	#apt-rpm)
	#	sudocmd apt-get autoclean
	#	;;
	apt-dpkg)
		sudocmd apt-get autoremove
		;;
	#yum-rpm)
	#	sudocmd yum clean all
	#	;;
	#urpm-rpm)
	#	sudocmd urpmi --clean
	#	;;
	#zypper-rpm)
	#	sudocmd zypper clean
	#	;;
	*)
		fatal "Do not known command for $PMTYPE"
		;;
esac

}
